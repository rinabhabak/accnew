type Query {
    getDistributors : [Distributors] @resolver(class: "\\Int\\DistributorGraphQl\\Model\\Resolver\\Distributors") @doc(description: "Get Distributors Details")
    getDistributorsAttributes : [Attributes]
    @resolver(class: "Int\\DistributorGraphQl\\Model\\Resolver\\Attributes") @doc(description: "Get Distributors Attribute Details")
    getDistributorsAttributesById(
    attributeId: Int @doc(description: "Attribute ID")
    ) : [AttributesById]
    @resolver(class: "Int\\DistributorGraphQl\\Model\\Resolver\\AttributesById") @doc(description: "Get Distributors Attribute Details By Id")
    filterDistributorsByAttribute(
        optionValue: String @doc(description: "Give ALl attributes")
    ) : [DistributorByOption]
    @resolver(class: "Int\\DistributorGraphQl\\Model\\Resolver\\FilterDistributorsByAttribute") @doc(description: "Get Distributors Attribute Details By by Attribute Option Values")  @cache(cacheIdentity: "Int\\DistributorGraphQl\\Model\\Resolver\\FilterDistributorsByAttribute\\Identity")
    filterDistributorsByLocation(
        optionValue: String @doc(description: "Give ALl attributes")
    ) : [DistributorByLocations]
    @resolver(class: "Int\\DistributorGraphQl\\Model\\Resolver\\FilterDistributorsByLocation") @doc(description: "Get Distributors Attribute Details By by Attribute Option Values") @cache(cacheIdentity: "Int\\DistributorGraphQl\\Model\\Resolver\\FilterDistributorsByLocation\\Identity")
}

