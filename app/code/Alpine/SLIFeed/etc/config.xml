<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <alpine_feed_export>
            <general>
                <sli_feed_cron_setting>0 * * * *</sli_feed_cron_setting>
                <sli_feed_cron_setting_enable>1</sli_feed_cron_setting_enable>
                <sli_feed_cron_excluded_pages>no-route,service-unavailable,media</sli_feed_cron_excluded_pages>
            </general>
        </alpine_feed_export>
    </default>
</config>
