<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="customer">
        <field name="customer_category" formElement="multiselect">
            <settings>
                <visible>true</visible>
            </settings>
        </field>
        <field name="pick_your_industry" formElement="select">
            <settings>
                <visible>true</visible>
            </settings>
        </field>
        <field name="other_industry" formElement="input">
            <settings>
                <visible>true</visible>
            </settings>
        </field>
    </fieldset>
</form>
